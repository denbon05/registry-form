{
  "name": "registry-form",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "fuser -k 3000/tcp |react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "CI=true npm test -- --env=jsdom --coverage",
    "format": "prettier --write .",
    "server": "fuser -k 9000/tcp | nodemon server/index.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn start\"",
    "droptable": "node bin/drop-table.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "proxy": "http://localhost:9000",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "Express": "^3.0.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "encrypt": "0.0.1",
    "express": "^4.17.1",
    "knex": "^0.21.15",
    "lodash": "^4.17.20",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "npm": "^6.14.11",
    "pg": "^8.5.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "web-vitals": "^0.2.4",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-node": "0.0.1-security",
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-classname": "^0.1.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "2.2.1"
  }
}
